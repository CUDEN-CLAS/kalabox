{
  "name": "kalabox",
  "version": "0.13.0",
  "main": "index.html",
  "private": true,
  "licenses": {
    "type": "MIT"
  },
  "window": {
    "title": "Kalabox",
    "toolbar": true,
    "frame": true,
    "width": 700,
    "height": 520,
    "max_width": 700,
    "max_height": 520,
    "min_width": 700,
    "min_height": 520,
    "resizeable": false,
    "icon": "./src/images/kalaboxv2-ico.png"
  },
  "platformOverrides": {
    "osx": {
      "window": {
        "toolbar": false,
        "frame": true
      }
    },
    "win": {
      "window": {
        "toolbar": false,
        "frame": true
      }
    },
    "linux": {
      "window": {
        "toolbar": false,
        "frame": true
      }
    }
  },
  "chromium-args": "--enable-threaded-compositing",
  "scripts": {
    "test": "grunt test"
  },
  "dependencies": {
    "decompress": "^3.0.0",
    "kalabox-cli": "https://github.com/kalabox/kalabox-cli/tarball/v0.12",
    "lodash": "^3.10.1",
    "lpad-align": "^1.0.0",
    "meow": "^3.3.0",
    "micromatch": "^2.3.7",
    "minimatch": "2 || 3",
    "moment": "^2.10.6",
    "multipipe": "^0.1.2"
  }
}
