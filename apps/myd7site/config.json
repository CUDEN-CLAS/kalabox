{
  "title": "My Drupal 7 Site",
  "desc": "A Drupal 7 site based on the Kalabox D7 profile.",
  "name": "myd7site",
  "components": {
    "data": {
      "image": "kalabox/data",
      "build": true,
      "src": "./dockerfiles/kalabox/data"
    },
    "web": {
      "image": "pirog/kalastack-docker",
      "proxy": [
        {
          "port": "80/tcp",
          "default": true
        }
      ]
    }
  }
}
