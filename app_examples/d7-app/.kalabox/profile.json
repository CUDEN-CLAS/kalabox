{
  "title": "Drupal 7 App",
  "name": "d7-app",
  "profile": "kalabox-profile-drupal7",
  "plugins": {
    "kalabox-plugin-drupal7": {}
  },
  "components": {
    "data": {
      "image": {
        "name": "kalabox/data-d7",
        "build": true,
        "src": "dockerfiles/kalabox/data-d7"
      }
    },
    "db": {
      "image": {
        "name": "kalabox/mariadb",
        "build": true,
        "src": "dockerfiles/kalabox/mariadb"
      }
    },
    "php": {
      "image": {
        "name": "kalabox/php-fpm",
        "build": true,
        "src": "dockerfiles/kalabox/php-fpm"
      }
    },
    "web": {
      "image": {
        "name": "kalabox/nginx",
        "build": true,
        "src": "dockerfiles/kalabox/nginx"
      },
      "proxy": [
        {
          "port": "80/tcp",
          "default": true
        }
      ]
    }
  }
}
